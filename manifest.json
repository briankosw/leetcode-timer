{
	"manifest_version": 2,
	"name": "leetcode-timer",
	"description": "Firefox extension for timing LeetCode problems",
	"version": "0.1.0",
	"icons": {
		"64": "./dist/images/light-timer.svg"
	},
	"content_scripts": [
		{
			"matches": [
				"*://leetcode.com/problems/*"
			],
			"js": ["./dist/content_script.js"],
			"css": [
				"./dist/css/styles.css"
			]
		}
	],
	"web_accessible_resources": ["./dist/images/*.svg"]
}